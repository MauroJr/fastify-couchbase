{
  "extends": ["airbnb-base"],
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script"
  },
  "rules": {
    "strict": ["error", "global"],
    "no-use-before-define": ["error", { "functions": false }],
    "comma-dangle": 0,
    "consistent-return": 0
  } 
}